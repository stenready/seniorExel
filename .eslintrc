{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "semi":"off",
    "max-len":"off",
    "require-jsdoc":"off",
    "linebreak-style": 0,
    "quotes": "off",
    "eol-last": "off"
  },
  "extends": ["eslint:recommended", "google"]
}
